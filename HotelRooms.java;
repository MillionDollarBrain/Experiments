import java.util.Scanner;
public class HotelRooms {
  private int[] rooms;
  private int remainingRooms;
  void setRemainingRooms() {
     private int result;
     for(i=0; i<rooms.length(); i++) {
        if(rooms[i] =! 0) {
           result++;
           } 
        }
     remainingRooms = result;
  }
  int[] getRooms(int key) {
    return rooms[key];
  }
  setRooms(int key, int value) {
    rooms[key] = value;
    setRemainngRooms();
  }
  int getRemainingRooms() {
    return remainingRooms;
  }
  public HotelRooms(int arraylen) {
    private rooms = new int[arraylen];
    while 0<1 {
    System.out.println("What is the functions you would like to perform? Type help to learn more");
      Scanner input = new Scanner(System.in);
      String str = input.next()
    }
  }
}
